<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="64dp"
        android:layout_marginTop="36dp"
        android:text="荷物受け取りアプリ トップ"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/image_button_touroku"
        android:layout_width="383dp"
        android:layout_height="230dp"
        android:layout_margin="50dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="71dp"
        android:background="@drawable/touroku"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/image_button_uketori"
        android:layout_width="393dp"
        android:layout_height="203dp"
        android:layout_margin="50dp"
        android:layout_marginStart="148dp"
        android:layout_marginTop="38dp"
        android:background="@drawable/uketori"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/image_button_touroku"
        tools:ignore="SpeakableTextPresentCheck" />

    <ListView
        android:id="@+id/event_show"
        android:layout_width="507dp"
        android:layout_height="329dp"
        android:layout_marginStart="144dp"
        android:layout_marginTop="34dp"
        app:layout_constraintStart_toEndOf="@+id/image_button_uketori"
        app:layout_constraintTop_toBottomOf="@+id/textView4">

    </ListView>

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="164dp"
        android:layout_marginTop="192dp"
        android:text="履歴"
        app:layout_constraintStart_toEndOf="@+id/image_button_touroku"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="648dp"
        android:layout_height="82dp"
        android:layout_marginStart="92dp"
        android:layout_marginTop="1148dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_select"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="84dp"
        android:layout_marginTop="1100dp"
        android:text="ryosei"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_select2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="208dp"
        android:layout_marginTop="1100dp"
        android:text="parcels"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <Button
        android:id="@+id/button_select3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="332dp"
        android:layout_marginTop="1100dp"
        android:text="event"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/duty_night_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="536dp"
        android:layout_marginTop="84dp"
        android:text="Night_Duty"
        app:layout_constraintStart_toEndOf="@+id/button_select3"
        app:layout_constraintTop_toBottomOf="@+id/event_show" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="484dp"
        android:layout_marginTop="64dp"
        android:background="@drawable/jimutoboder"
        android:orientation="horizontal"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/main_jimutou_show"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:text="設定されていません。"
            android:textSize="20sp"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_editor_absoluteX="740dp" />

        <Button
            android:id="@+id/jimuto_change_button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:text="jimutou_change"
            app:layout_constraintTop_toTopOf="parent"
            app:rippleColor="@color/teal_200"
            tools:layout_editor_absoluteX="1009dp" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>